{
    "name": "Randomkey",
    "version": "1.0",
    "description": "A Chrome extension that generates a random password and stores it in a table.",
    "manifest_version": 3,
    "permissions": [
        "clipboardWrite",
        "storage",
        "tabs"
    ],
    "action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}